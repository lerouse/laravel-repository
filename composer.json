{
    "name": "lerouse/laravel-repository",
    "description": "Repository implementation for Laravel.",
    "keywords": ["laravel", "eloquent", "database", "repository"],
    "type": "package",
    "license": "MIT",
    "authors": [
        {
            "name": "Christian Innerbichler"
        }
    ],
    "require": {
        "php": "^8.0|^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/events": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/database": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^6.0|^7.0|^8.0|^9.0|^10.0",
        "laravel/legacy-factories": "^1.2"
    },
    "require-dev": {
        "mockery/mockery": "~1.0",
        "orchestra/testbench": "^4.0||^5.0||^6.0||^7.0||^8.0",
        "phpunit/phpunit": "^7.3||^8.2||^9.3||^10.0"
    },
    "autoload": {
        "psr-4": {
            "Lerouse\\LaravelRepository\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Lerouse\\LaravelRepository\\Tests\\": "tests"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "laravel": {
            "providers": [
                "Lerouse\\LaravelRepository\\LaravelRepositoryServiceProvider"
            ]
        }
    }
}
